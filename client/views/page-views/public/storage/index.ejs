<%- include('../../../../components/header.ejs') %>
  <div class="ashe-public-storage">
    <article >
      <!-- banner -->
      <section class="content-banner">
        <div class="type-area">
          <div class="banner">
            <div class="banner-title"><%= banner_title %></div>
            <div class="banner-brief"><%= banner_brief %></div>
            <div class="banner-describe"><%= banner_describe %></div>
            <div class="banner-describe-mobile"><%= banner_describe_mobile %></div>
          </div>
        </div>
        <div class="banner-img" style="background:url(/static/assets/page-views/public/storage/banner.jpg) no-repeat center center;background-size:cover"></div>
        <!-- <img class="banner-img-mobile" src="/static/assets/page-views/public/storage/banner-m.png"> -->
      </section>
      <div class="breadcrumb">
        <ul class="breadcrumb-center">
          <li class="first"><a class="firstpage" href="/"><%= breadcrumb_one %></a>&nbsp;/&nbsp;</li>
          <li class="second"><a class="success" href="/public/compute"><%= breadcrumb_two %></a>&nbsp;/&nbsp;</li>
          <li class="third"><%= breadcrumb_three %></li>
        </ul>
      </div>
      <!-- end for banner -->
      <!-- content -->
      <div class="content-main">
        <div class="volume-center">
          <div class="introduction">
            <img src="/static/assets/page-views/public/storage/volume.png">
            <div class="right">
              <p class="shuiyin">VOLUMES</p>
              <p class="title"><%= introduction_title_one %><span><%= introduction_title_two %></span></p>
              <p class="intro"><%= introduction_intro %></p>
              <a class="signup" href="http://console.tfcloud.com"><%= introduction_a %></a>
            </div>
            <div class="description">
              <div class="description_item">
                <img src="/static/assets/page-views/public/storage/1.png">
                <p class="line"></p>
                <p class="description_title"><%= description_title_one %></p>
                <p class="description_brief"><%= description_brief_one %></p>
              </div>
              <div class="description_item">
                <img src="/static/assets/page-views/public/storage/2.png">
                <p class="line"></p>
                <p class="description_title"><%= description_title_two %></p>
                <p class="description_brief"><%= description_brief_two %></p>
              </div>
              <div class="description_item">
                <img src="/static/assets/page-views/public/storage/3.png">
                <p class="line"></p>
                <p class="description_title"><%= description_title_three %></p>
                <p class="description_brief"><%= description_brief_three %></p>
              </div>
              <div class="description_item">
                <img src="/static/assets/page-views/public/storage/4.png">
                <p class="line"></p>
                <p class="description_title"><%= description_title_four %></p>
                <p class="description_brief"><%= description_brief_four %></p>
              </div>
              <div class="description_item">
                <img src="/static/assets/page-views/public/storage/5.png">
                <p class="line"></p>
                <p class="description_title"><%= description_title_five %></p>
                <p class="description_brief"><%= description_brief_five %></p>
              </div>
              <div class="description_item">
                <img src="/static/assets/page-views/public/storage/6.png">
                <p class="line"></p>
                <p class="description_title"><%= description_title_six %></p>
                <p class="description_brief"><%= description_brief_six %></p>
              </div>
              <div class="description_item">
                <img src="/static/assets/page-views/public/storage/7.png">
                <p class="line"></p>
                <p class="description_title"><%= description_title_seven %></p>
                <p class="description_brief"><%= description_brief_seven %></p>
              </div>
            </div>
            <div class="pc-function">
              <div class="function-item">
                <img class="function-img" src="/static/assets/page-views/public/storage/1.png">
                <p class="line"></p>
                <p class="function-title"><%= description_title_one %></p>
                <p class="function-intro"><%= description_brief_one %></p>
              </div>
              <div class="function-item">
                <img class="function-img" src="/static/assets/page-views/public/storage/2.png">
                <p class="line"></p>
                <p class="function-title"><%= description_title_two %></p>
                <p class="function-intro"><%= description_brief_two %></p>
              </div>
              <div class="function-item">
                <img class="function-img" src="/static/assets/page-views/public/storage/3.png">
                <p class="line"></p>
                <p class="function-title"><%= description_title_three %></p>
                <p class="function-intro"><%= description_brief_three %></p>
              </div>
            </div>
            <a class="signup-mobile" href="http://console.tfcloud.com"><%= introduction_a %></a>
          </div>
        </div>
      </div>
      <div class="content-product">
        <div class="product-center">
          <div class="product-title"><%= product_title_one %><span><%= product_title_two %></span></div>
          <div class="feature">
            <div class="mobile-top">
              <img class="block-mobile-one" src="/static/assets/page-views/public/storage/bg.jpg">
              <img class="block-mobile-two" src="/static/assets/page-views/public/storage/bg2.jpg">
              <img class="block-mobile-three" src="/static/assets/page-views/public/storage/bg3.jpg">
              <img class="block-mobile-four" src="/static/assets/page-views/public/storage/bg4.jpg">
            </div>
            <div class="feature-top">
              <div class="feature-item feature-item-one">
                <img src="/static/assets/page-views/public/storage/product1.png">
                <p class="item-title"><%= feature_title_one %></p>
                <p class="item-brief"><%= feature_brief_one %></p>
              </div>
              <div class="feature-item feature-item-two">
                <img src="/static/assets/page-views/public/storage/product2.png">
                <p class="item-title"><%= feature_title_two %></p>
                <p class="item-brief"><%= feature_brief_two %></p>
              </div>
              <div class="feature-item feature-item-three">
                <img src="/static/assets/page-views/public/storage/product3.png">
                <p class="item-title"><%= feature_title_three %></p>
                <p class="item-brief"><%= feature_brief_three %></p>
              </div>
              <div class="feature-item feature-item-four">
