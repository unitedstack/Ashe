                <p class="item-brief"><%= feature_brief_four %></p>
              </div>
            </div>
            <div class="feature-bottom">
              <img class="block-one" src="/static/assets/page-views/public/storage/bg.jpg">
              <img class="block-two" src="/static/assets/page-views/public/storage/bg2.jpg">
              <img class="block-three" src="/static/assets/page-views/public/storage/bg3.jpg">
              <img class="block-four" src="/static/assets/page-views/public/storage/bg4.jpg">
            </div>
          </div>
        </div>
      </div>
      <!-- end for content -->
    </div>
    </article>
<%- include('../../../../components/footer.ejs') %>