<%- include('../../../../components/header.ejs') %>
  <div class="ashe-product-storage">
    <article >
      <!-- banner -->
      <section class="content-banner">
        <div class="type-area">
          <div class="banner">
            <div class="banner-title"><%= banner_title %></div>
            <div class="banner-brief"><%= banner_brief %></div>
            <div class="banner-describe"><%= banner_describe %></div>
            <div class="banner-describe-mobile"><%= banner_describe_mobile %></div>
          </div>
        </div>
        <div class="banner-img" style="background:url(/static/assets/page-views/product/storage/banner2.jpg) no-repeat center center;background-size:cover"></div>
        <!-- <img class="banner-img-mobile" src="/static/assets/page-views/product/storage/banner-m.png"> -->
      </section>
      <div class="breadcrumb">
        <ul class="breadcrumb-center">
          <li class="first"><a class="firstpage" href="/"><%= breadcrumb_one %></a>&nbsp;/&nbsp;</li>
          <li class="second"><a class="success" href="/product/environment"><%= breadcrumb_two %></a>&nbsp;/&nbsp;</li>
          <li class="third"><%= breadcrumb_three %></li>
        </ul>
      </div>
      <!-- main -->
      <div class="content-main">
        <div class="part-one">
          <p class="shuiyin">STORAGE</p>
          <p class="title"><%= storage_title_one %><span><%= storage_title_two %></span></p>
          <p class="brief"><%= storage_intro %></p>
          <div class="performance">
            <div class="performance-item">
              <img src="/static/assets/page-views/product/storage/1.png">
              <p class="performance-title"><%= performance_title_one %></p>
              <div class="performance-intro">
                <p><%= performance_intro_one_first %></p>
                <p><%= performance_intro_one_second %></p>
                <p><%= performance_intro_one_third %></p>
              </div>
            </div>
            <div class="performance-item">
              <img src="/static/assets/page-views/product/storage/2.png">
              <p class="performance-title"><%= performance_title_two %></p>
              <div class="performance-intro">
                <p><%= performance_intro_two_first %></p>
                <p><%= performance_intro_two_second %></p>
                <p><%= performance_intro_two_third %></p>
              </div>
            </div>
            <div class="performance-item">
              <img src="/static/assets/page-views/product/storage/3.png">
              <p class="performance-title"><%= performance_title_three %></p>
              <div class="performance-intro">
                <p><%= performance_intro_three_first %></p>
                <p><%= performance_intro_three_second %></p>
                <p><%= performance_intro_three_third %></p>
                <p><%= performance_intro_three_fourth %></p>
              </div>
            </div>
            <div class="performance-item">
              <img src="/static/assets/page-views/product/storage/4.png">
              <p class="performance-title"><%= performance_title_four %></p>
              <div class="performance-intro">
                <p><%= performance_intro_four_first %></p>
                <p><%= performance_intro_four_second %></p>
                <p><%= performance_intro_four_third %></p>
              </div>
            </div>
          </div>
          <div class="framework">
            <img src="/static/assets/page-views/product/storage/framework1@v2.0.png">
          </div>
        </div>
        <div class="part-two">
          <p class="shuiyin">FAST STORAGE</p>
          <p class="title"><%= fast_storage_title_one %><span><%= fast_storage_title_two %></span></p>
          <p class="brief"><%= fast_storage_intro %></p>
          <div class="feature">
            <div class="item">
              <img src="/static/assets/page-views/product/storage/storage1.png">
              <p class="item_title"><%= storage_feature_title_one %></p>
              <p class="item_intro"><%= storage_feature_intro_one %></p>
            </div>
            <div class="item">
              <img src="/static/assets/page-views/product/storage/storage2.png">
              <p class="item_title"><%= storage_feature_title_two %></p>
              <p class="item_intro"><%= storage_feature_intro_two %></p>
            </div>
            <div class="item">
              <img src="/static/assets/page-views/product/storage/storage3.png">
              <p class="item_title"><%= storage_feature_title_three %></p>
              <p class="item_intro"><%= storage_feature_intro_three %></p>
            </div>
          </div>
          <div class="performance">
            <div class="performance-item">
              <img src="/static/assets/page-views/product/storage/5.png">
              <p class="performance-title"><%= part_two_performance_title_one %></p>
              <div class="performance-intro"><%= part_two_performance_intro_one %></div>
            </div>
            <div class="performance-item">
              <img src="/static/assets/page-views/product/storage/6.png">
              <p class="performance-title"><%= part_two_performance_title_two %></p>
              <div class="performance-intro"><%= part_two_performance_intro_two%></div>
            </div>
            <div class="performance-item">
              <img src="/static/assets/page-views/product/storage/7.png">
              <p class="performance-title"><%= part_two_performance_title_three %></p>
              <div class="performance-intro"><%= part_two_performance_intro_three %></div>
            </div>
            <div class="performance-item">
              <img src="/static/assets/page-views/product/storage/8.png">
              <p class="performance-title"><%= part_two_performance_title_four %></p>
              <div class="performance-intro"><%= part_two_performance_intro_four %></div>
            </div>
          </div>
        </div>
        <div class="part-three">
          <p class="shuiyin">OBJECT STORAGE</p>
          <p class="title"><%= object_storage_title_one %><span><%= object_storage_title_two %></span></p>
          <p class="brief"><%= object_storage_intro %></p>
          <div class="feature">
            <div class="item">
              <img src="/static/assets/page-views/product/storage/storage4.png">
              <p class="item_title"><%= object_storage_feature_title_one %></p>
              <p class="item_intro"><%= object_storage_feature_intro_one %></p>
            </div>
            <div class="item">
              <img src="/static/assets/page-views/product/storage/storage5.png">
              <p class="item_title"><%= object_storage_feature_title_two %></p>
              <p class="item_intro"><%= object_storage_feature_intro_two %></p>
            </div>
            <div class="item">
              <img src="/static/assets/page-views/product/storage/storage6.png">
              <p class="item_title"><%= object_storage_feature_title_three %></p>
              <p class="item_intro"><%= object_storage_feature_intro_three %></p>
            </div>
          </div>
          <div class="framework">
            <img src="/static/assets/page-views/product/storage/framework2@v2.0.png">
          </div>
        </div>
      </div>
    </article>
  </div>
<%- include('../../../../components/footer.ejs') %>