              <div class="ani3"></div>
              <div class="ani4"></div>
              <div class="ani5"></div>
              <div class="ani6"></div>
              <div class="ani7"></div>
              <div class="ani8"></div>
              <div class="ani9"></div>
              <div class="ani10"></div>
              <div class="ani11"></div>
            </div>
          </div>
        </li>
        <li class="feature-wrapper">
          <div class="feature-content-wrapper clearfix">
            <div class="clearfix feat-content">
              <div class="feature-title">
                <%= third_feat_title %>
              </div>
              <div class="feature-content">
                <%= third_feat_content %>
              </div>
            </div>
            <div class="feat-ani">
