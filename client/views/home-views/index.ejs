                  <div class="ani4"></div>
                  <div class="ani5"></div>
                  <div class="ani6"></div>
                  <div class="ani7"></div>
                  <div class="ani8"></div>
                  <div class="ani9"></div>
                  <div class="ani10"></div>
                  <div class="ani11"></div>
                </div>
            </div>
          </li>
      </ul>
    </div>
    <!-- features end -->

    <!-- area begin -->
    <div class="area-wrapper">
      <div class="area-title">
        <div class="bg-title">SERVICE AREA</div>
        <div class="title">
          <span><%= area_title_sec1 %></span><span><%= area_title_sec2 %></span>
        </div>
      </div>
      <div><%= area_intro_content %></div>
      <ul class="area-list clearfix">
        <li class="area-content-wrapper" data-index=1>
          <div class="top">
            <img src="/static/assets/home-views/edu.png" alt="" class="area-icon">
            <div class="bar"></div>
            <div class="area-name"><%= first_area_name %></div>
          </div>
          <div class="bottom">
