<%- include('../../components/header.ejs') %>
<div class="main">
  <!-- banner begin -->
  <section class="banner">
    <div class="pc-banner">
      <ul class="carousel">
        <li class="banner-wrapper active">
          <div class="banner-content-wrapper">
            <div class="banner-title1"><%= first_banner_title1 %></div>
            <div class="banner-title2"><%= first_banner_title2 %></div>
            <div class="banner-content"><%= first_banner_content %></div>
            <div class="clearfix">
              <a href="/public/storage">
                <button class="banner-btn1 banner-btn"><%= banner_btn1 %></button>
              </a>
              <a href="http://console.tfcloud.com/register?next=/">
                <button class="banner-btn2 banner-btn"><%= banner_btn2 %></button>
              </a>
            </div>
          </div>
        </li>
        <li class="banner-wrapper">
          <div class="banner-content-wrapper">
            <div class="banner-title1"><%= second_banner_title1 %></div>
            <div class="banner-title2"><%= second_banner_title2 %></div>
            <div class="banner-content"><%= second_banner_content %></div>
            <div class="clearfix">
              <a href="/product/cloud-security">
                <button class="banner-btn1 banner-btn"><%= banner_btn1 %></button>
              </a>
              <a href="http://console.tfcloud.com/register?next=/">
                <button class="banner-btn2 banner-btn"><%= banner_btn2 %></button>
              </a>
            </div>
          </div>
        </li>
        <li class="banner-wrapper">
          <div class="banner-content-wrapper">
            <div class="banner-title1"><%= third_banner_title1 %></div>
            <div class="banner-title2"><%= third_banner_title2 %></div>
            <div class="banner-content"><%= third_banner_content %></div>
            <div class="clearfix">
              <a href="/product/cloud-platform/product">
                <button class="banner-btn1 banner-btn"><%= banner_btn1 %></button>
              </a>
              <a href="http://console.tfcloud.com/register?next=/">
                <button class="banner-btn2 banner-btn"><%= banner_btn2 %></button>
              </a>
            </div>
          </div>
        </li>
      </ul>
      <ul class="control-bar">
        <li class="point active" data-index=0></li>
        <li class="point" data-index=1></li>
        <li class="point" data-index=2></li>
      </ul>
    </div>
  </section>
  <!-- banner end -->

  <!-- content begin -->
  <section class="content">
    
    <!-- architecture begin -->
    <div class="architecture clearfix">
      <div class="content-wrapper">
        <div class="content-title"><%= architecture_title %></div>
        <div class="architecture-content"><%= architecture_content %></div>
        <a href="/product/environment">
          <button class="content-btn"><%= architecture_btn %></button>
        </a>
      </div>
      <div class="img-wrapper">
