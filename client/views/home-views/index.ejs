          </div>
        </li>
        <li class="area-content-wrapper" data-index=3>
          <div class="top">
            <img src="/static/assets/home-views/gov.png" alt="" class="area-icon">
            <div class="bar"></div>
            <div class="area-name"><%= third_area_name %></div>
          </div>
          <div class="bottom">
            <div class="content-title"><%= third_area_name %></div>
            <div class="area-content"><%= third_area_content %></div>
          </div>
        </li>
        <li class="area-content-wrapper" data-index=4>
          <div class="top">
            <img src="/static/assets/home-views/com.png" alt="" class="area-icon">
            <div class="bar"></div>
            <div class="area-name"><%= fourth_area_name %></div>
          </div>
          <div class="bottom">
            <div class="content-title"><%= fourth_area_name %></div>
            <div class="area-content"><%= fourth_area_content %></div>
          </div>
        </li>
      </ul>
      <!-- <button><%= area_btn %></button> -->
    </div>
    <!-- area end -->

    <!-- customer begin -->
    <div class="customer-wrapper">
      <div class="customer-title title-section">
        <div class="bg-title">CUSTOMER</div>
        <div class="title">
          <span><%= customer_title_sec1 %></span><span><%= customer_title_sec2 %></span>
        </div>
      </div>
      <div><%= customer_append_content %></div>
      <ul class="customer-list clearfix">
        <% for(var i = 1; i <= 10; i++) { %>
          <li class="customer customer<%= i %>">
            <!-- <img src="/static/assets/home-views/cu<%= i %>.png" alt=""> -->
          </li>
        <% } %>  
        </ul>
      <!-- <button>更多客户</button> -->
