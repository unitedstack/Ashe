          </div>
        </div>
        <div class="feature-content"><%= first_feat_content %></div>
      </div>
      <ul class="feature-list">
        <li class="feature-wrapper">
          <div class="feature-content-wrapper clearfix">
            <div class="clearfix feat-content">
                <div class="feature-title">
                    <%= second_feat_title %>
                  </div>
                  <div class="feature-content">
                    <%= second_feat_content %>
                  </div>
            </div>
            <div class="feat-ani">
